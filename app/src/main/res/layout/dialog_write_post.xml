<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="265dp"
    android:layout_height="159dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_gravity="center"
    android:background="@drawable/bg_rectangle_white_8">

    <TextView
        android:id="@+id/dialog_total_title_tv"
        android:layout_width="146dp"
        android:layout_height="57dp"
        android:text="글이 게시되었어요."
        android:textSize="16sp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_marginTop="11dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/dialog_total_choose1_tv"
        android:layout_width="231dp"
        android:layout_height="29dp"
        android:text="내가 쓴 글 보러가기"
        android:textColor="@color/white"
        android:textSize="14sp"
        android:textStyle="bold"
        android:gravity="center"
        android:background="@drawable/bg_rectangle_orange_24"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@id/dialog_total_title_tv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/dialog_total_choose2_tv"
        android:layout_width="231dp"
        android:layout_height="29dp"
        android:text="닫기"
        android:textColor="@color/black"
        android:textSize="10sp"
        android:textStyle="bold"
        android:gravity="center"
        android:layout_marginTop="5dp"
        android:background="@drawable/bg_rectangle_white_stroke_gray60_1px"
        app:layout_constraintTop_toBottomOf="@id/dialog_total_choose1_tv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>