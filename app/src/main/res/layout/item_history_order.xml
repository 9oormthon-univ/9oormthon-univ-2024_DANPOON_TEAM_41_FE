<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/bg_rectangle_white_8"
    android:layout_marginStart="11dp"
    android:layout_marginEnd="11dp"
    android:layout_marginTop="9dp"
    android:layout_marginBottom="9dp">

    <TextView
        android:id="@+id/item_history_order_name_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="고양이네 생선가게"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_marginTop="18dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/item_history_order_date_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2024.10.24"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        android:layout_marginTop="23dp"
        android:layout_marginStart="6dp"
        app:layout_constraintStart_toEndOf="@id/item_history_order_name_tv"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/item_history_order_item_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="갈치,고등어 외2. . ."
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        android:layout_marginTop="14dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintTop_toBottomOf="@id/item_history_order_name_tv"
        app:layout_constraintStart_toStartOf="@id/item_history_order_name_tv"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>